{
  "plugins": [
    [
      "postcss-preset-env",
      {
        "browsers": "last 2 versions",
        "features": {
          "color-mod-function": { "unresolved": "ignore" },
          "custom-media-queries": true,
          "media-query-ranges": true
        },
        "importFrom": [
          "./src/config/variables.css"
        ],
        "preserve": false,
        "stage": 2
      }
    ],
    "postcss-nested"
  ]
}
